{
  "name": "kedai",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "rm -rf ./apps/mobile/assets/locales/* && cp -r ./packages/locale/src/locales/* ./apps/mobile/assets/locales && dotenv -- turbo run dev --parallel",
    "clean": "find . -name node_modules -o -name .next -o -name .expo -type d -prune | xargs rm -rf",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "dotenv -- turbo run db:push"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "prettier": "latest",
    "tailwindcss": "^3.4.13",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.11.0"
}
