{
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".expo/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "env": [
        "CLOUDFLARE_ACCESS_ID",
        "CLOUDFLARE_ACCESS_SECRET",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_IMAGE_BASE_PATH",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "DATABASE_URL",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "NEXT_PUBLIC_GOOGLE_AUTH_ID",
        "GOOGLE_AUTH_SECRET",
        "NEXT_PUBLIC_BETTER_AUTH_URL",
        "BETTER_AUTH_SECRET"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ],
      "env": [
        "CLOUDFLARE_ACCESS_ID",
        "CLOUDFLARE_ACCESS_SECRET",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_IMAGE_BASE_PATH",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "DATABASE_URL",
        "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "NEXT_PUBLIC_GOOGLE_AUTH_ID",
        "GOOGLE_AUTH_SECRET",
        "NEXT_PUBLIC_BETTER_AUTH_URL",
        "BETTER_AUTH_SECRET"
      ]
    },
    "lint": {
      "outputs": []
    },
    "db:push": {
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ],
      "env": [
        "BETTER_AUTH_SECRET",
        "CLOUDFLARE_ACCESS_ID",
        "CLOUDFLARE_ACCESS_SECRET",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_IMAGE_BASE_PATH",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "GOOGLE_AUTH_SECRET",
        "DATABASE_URL"
      ]
    },
    "postinstall": {
      "outputs": []
    }
  }
}
